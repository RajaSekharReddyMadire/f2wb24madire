extends layout

block content
  h1= title
  - var image_names = ["husky.jpg", "bulldog.jpg", "germanshepard.jpg", "goldenretreiver.jpg", "rottweiler.jpg"];
    - for(let i = 0; i < 3; i++) {
        - let ranValue = Math.floor(Math.random() * 5);
        img(src=`/images/${image_names[ranValue]}`, alt="Dog Image", height="300px", width="300px")
    - }